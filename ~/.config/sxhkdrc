Print
    import png:- | xclip -selection clipboard -t image/png -i #take screenshot and save it in clipboard

super + shift + e 
#report about batery
    xsetroot -name "`upower -i /org/freedesktop/UPower/devices/battery_BAT0 | grep -e 'time to empty' -e percentage`" #naslov naprave (baterije) pridobi≈° z ukazom `uppower -e`

super + shift + d
# show date and time
    xsetroot -name "`date +'%d.%m.%Y, %H:%M'`"; sleep 2; xsetroot -name "`date +%H:%M`"

super + shift + a
#run pulsemixer for sound settings
    st -e pulsemixer 

super + e
#run LF (List File)
    st -e lf
